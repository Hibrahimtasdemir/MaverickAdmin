<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name=" test suite"  parallel="tests" thread-count="5">
    <!-- parallel="tests" thread-count="3" -->

    <listeners>
        <listener class-name="amazon.tests.ExtentReportListener"/>
        <listener class-name="amazon.tests.AllureReportListener"></listener>
    </listeners>


    <test name="JSON Tests ">
        <classes>
            <class name="amazon.tests.JsonDataTest"/>
        </classes>
    </test>

    <test name="CSV Tests ">
        <classes>
            <class name="amazon.tests.ExcelDataCsvTest"></class>
        </classes>
    </test>

    <test name="amazon tests">
        <classes>
            <class name="amazon.tests.US001_Homepage"></class>
        </classes>
    </test>

</suite>

